{
    "name": "phpwatch/fast404",
    "description": "A PSR-7 middleware to return 404 errors fast for requests that are not worth returning a fancy 404 error.",
    "type": "library",
    "require": {
        "php": "^8.3",
        "psr/http-message": "^1.1 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5.3",
        "nyholm/psr7": "^1.8.1"
    },
	"autoload": {
        "psr-4": {
            "PHPWatch\\Fast404\\": "src/"
        }
    },
	"autoload-dev": {
        "psr-4": {
            "PHPWatch\\Fast404\\Tests\\": "tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ayesh Karunaratne",
            "email": "ayesh@aye.sh"
        }
    ],
    "minimum-stability": "stable"
}
